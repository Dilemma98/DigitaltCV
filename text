let buttonAbt = document.getElementById("aboutMe");
let buttonWrk = document.getElementById("workExp");
let navBtns = document.getElementById("navBtns");
let h3Abt = document.createElement("h3");
h3Abt.innerText = "Om mig";
let pAbt = document.createElement("p");
pAbt.innerText = "Hej! Emma Högdal heter jag och har i augusti -24 påbörjat min studieresa inom webbutveckling. Tidigare har jag varit kommunalt anställd som löneadministratör, men även som barnskötare. Utöver det kommunala har jag också varit anställd hos PostNord. Mitt mål för framtiden är just nu att slutföra min utbildning så att jag så småningom kan arbeta inom denna branch.";
let h3Wrk = document.createElement("h3");
h3Wrk.innerText = "Tidigare arbetslivserfarenhet";
let pWrk = document.createElement("p");
pWrk.innerText = "2023-2024 Löneadministratör, Kumla kommun 2023 Löneadministratör, behovsanställd 2023 Barnskötare 2022 Föräldraledig 2019-2021 Brevbärare, PostNord"
        


function aboutMe(){
    document.body.appendChild(h3Abt);
    document.body.appendChild(pAbt);
    document.body.removeChild(h3Wrk);
    document.body.removeChild(pWrk);
}

function workExp(){
    document.body.appendChild(h3Wrk);
    document.body.appendChild(pWrk);
    document.body.removeChild(h3Abt);
    document.body.removeChild(pAbt);
}